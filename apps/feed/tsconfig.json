{
  "extends": "@feedgot/tsconfig/nextjs.json",
  "references": [
    { "path": "../../packages/db" }
  ],
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "composite": true,
    "esModuleInterop": true,
    "noUncheckedIndexedAccess": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "types": [
      "@cloudflare/workers-types/2023-07-01"
    ],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "target": "ES2017",
    "noEmit": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "**/*.d.ts"
, "../../packages/db/drizzle.config.ts", "../../packages/ui/src/components/toggle-group.tsx", "../../packages/ui/src/components/sonner.tsx", "../../packages/ui/src/components/sidebar.tsx", "../../packages/ui/src/components/chart.tsx", "../../packages/ui/src/components/drawer.tsx", "../../packages/ui/src/components/checkbox.tsx", "../../packages/ui/src/components/breadcrumb.tsx", "../../packages/ui/src/components/table.tsx", "../../packages/ui/src/components/sheet.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
